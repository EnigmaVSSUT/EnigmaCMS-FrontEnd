{
  "compilerOptions": {
	"module": "ES6",
	"target": "ES6",
    "baseUrl": ".",
	"moduleResolution": "node",
    "paths": {
      "@/components/*": ["./src/components/*"],
      "@/pages/*": ["./src/pages/*"],
	  "@/styles/*": ["./src/styles/*"],
	  "@/lib/*": ["./src/lib/*"],
	  "@/layout/*": ["./src/layout/*"],
    "@/theme/*": ["./src/theme/*"],
    "@/router/*": ["./src/router/*"],
    }
  },
  "exclude": ["node_modules"]
}
